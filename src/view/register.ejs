
  
<!DOCTYPE html>
<html lang="en">
	<head>
		<% include partials/head %>
	</head>
	<body class="container">
		<div class="col-md-6 col-md-offset-3">
            <h1>SIGN UP</h1>
        	</body>
</html>

<div id="form">
    <hr/>
    <form action="/register" method="post">
      <div class="form-group">
        <label for="form_username">Username</label>
        <input id="form_username" type="text" name="username" required/>
      </div>
      <div class="form-group">
        <label for="form_mail">Mail</label>
        <input id="form_mail" type="text" name="mail" required/>
      </div>
      <div class="form-group">
        <label for="form_password">Password</label>
        <input id="form_password" type="password" name="password" required />
      </div>
      <button class="btn btn-primary btn-block" type="submit" value="Submit">Register</button>
    </form>
    <hr/>
    
  </div>

  <a href="/login">Login</a>